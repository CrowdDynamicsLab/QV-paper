      \field{journaltitle}{Annual Review of Sociology}
      \field{title}{Human Motivation and Social Cooperation: Experimental and Analytical Foundations}
      \field{volume}{33}
      \field{year}{2007}
      \field{pages}{pp. 43\bibrangedash 64}
